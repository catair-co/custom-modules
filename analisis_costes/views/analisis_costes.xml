    <!--    Form  -->
    <record id="analisis_de_costes_view_form" model="ir.ui.view">
        <field name="model">analisis_de_costes</field>
        <field name="arch" type="xml">
            <form>
                <header/>
                <sheet string="analisis">
                    <div class="oe_title">
                        <h1>
                            <field name="name" required="1" placeholder="Name..."/>
                        </h1>
                    </div>
                    <group name="grupo1">
                        <group name="grupo1_left">
                            <field name="user_id" widget="many2one_avatar_user"/>
                        </group>
                        <group name="grupo1_right">
                            <field name="activo" widget="boolean_toggle"/>
                        </group>
                    </group>
                    <group>
                        <field name="notes" placeholder="Escriba sus notas aquí ..." nolabel="0"/>
                    </group>
                    <!--
                    <div class="oe_chatter" name="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="message_ids"/>
                        <field name="activity_ids"/>
                    </div>
                    -->
                    <notebook name="nbook1">
                        <page string="Info General" name="pest1">
                            <group name="pest1">
                                <group name="pest1_left">
                                    <field name="cliente"/>
                                    <field name="nombre_producto"/>
                                    <field name="fecha"/>
                                    <field name="aplicacion"/>
                                    <field name="pais"/>
                                    <field name="fecha_lanzamiento"/>
                                    <field name="comentarios"/>
                                </group>
                                <group name="pest1_right">
                                    <field name="producto_existente"/>
                                    <field name="unidades_pedido"/>
                                    <field name="unidades_anual"/>
                                    <field name="precio_target"/>
                                    <field name="velocidad_encapsulado"/>
                                    <field name="velocidad_envasado"/>
                                    <field name="merma_estimada"/>
                                    <field name="tipo_limpieza"/>
                                </group>
                            </group>
                        </page>
                        <page string="Packaging Primario" name="pest2">
                            <group name="pest2">
                                <field name="packaging_primario"/>
                            </group>
                            <notebook name="pprimario">
                                <page string="Blister" name="blister"
                                      attrs="{'invisible': [['packaging_primario','!=','blister']]}">
                                    <group name="blister">
                                        <field name="tamanyo_blister"/>
                                        <field name="blister_unidad"/>
                                        <field name="serigrafia"/>
                                        <field name="bobina_aluminio"/>
                                    </group>
                                </page>
                                <page string="Pildorero" name="pildorero"
                                      attrs="{'invisible': [['packaging_primario','!=','pildorero']]}">
                                    <group name="pildorero">
                                        <field name="tamanyo_pet"/>
                                        <field name="tapa"/>
                                        <field name="loteado"/>
                                        <field name="color"/>
                                        <field name="etiqueta"/>
                                    </group>
                                </page>
                            </notebook>
                        </page>
                        <page string="Packaging Secundario" name="pest3">
                            <group name="pest3">
                                <group name="pest3_left">
                                    <field name="estuche"/>
                                    <field name="especificaciones_estuche"/>
                                    <field name="prospecto"/>
                                    <field name="formato_lote"/>
                                </group>
                                <group name="pest3_right">
                                </group>
                            </group>
                        </page>
                        <page string="Galénica/Analítica" name="pest4">
                            <group name="pest4">
                                <group name="pest4_left">
                                    <field name="galenica"/>
                                    <field name="producto_galenica"/>
                                    <field name="unidad_por_envase"/>
                                </group>
                                <group name="pest4_right">
                                    <field name="lote"/>
                                    <field name="analisis_especiales"/>
                                    <field name="especificar_analitica"/>
                                </group>
                            </group>
                        </page>
                        <page string="Ingredientes" name="pest5">
                            <group name="pest5">
                                <field name="ingredientes_ids" nolabel="1">
                                    <tree editable="bottom">
                                        <field name="origen"/>
                                        <field name="ingrediente"/>
                                        <field name="mg_unitaria"/>
                                        <field name="fuente"/>
                                        <field name="vrn"/>
                                        <field name="marca"/>
                                        <field name="alternativa"/>
                                        <field name="proveedor" context="{'an_line_search_product':ingrediente}"/>
                                        <field name="price"/>
                                        <field name="analisis_de_costes_id" invisible="1"/>
                                        <field name="company_id" invisible="1"/>
                                        <field name="company_currency_id" invisible="1"/>
                                    </tree>
                                </field>
                            </group>
                        </page>
                        <page string="Excipientes" name="pest6">
                            <group name="pest6">
                                <field name="excipientes_ids" nolabel="1">
                                    <tree editable="bottom">
                                        <field name="origen"/>
                                        <field name="excipiente"/>
                                        <field name="mg_unitaria"/>
                                        <field name="proveedor" context="{'an_line_search_product':excipiente}"/>
                                        <field name="price"/>                                        
                                        <field name="analisis_de_costes_id" invisible="1"/>
                                        <field name="company_id" invisible="1"/>
                                        <field name="company_currency_id" invisible="1"/>
                                    </tree>
                                </field>
                            </group>
                        </page>
                        <page string="Otros" name="pest7">
                            <group name="pest7">
                                <field name="aspecto_visual"/>
                                <field name="limitacion_excipientes"/>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!--    Acción  -->
    <record id="action_analisis_de_costes" model="ir.actions.act_window">
        <field name="name">Análisis de Costes</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">analisis_de_costes</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Cree un nuevo Análisis de Costes para empezar a trabajar
            </p>
        </field>
    </record>

    <!--    Menú
    Para Project:
    parent="project.menu_main_pm"
    ó
    parent="documents.menu_root"
    -->
    <menuitem id="menu_analisis"
              name="Análisis de Costes"
              parent="documents.menu_root"
              action="action_analisis_de_costes"
              sequence="99"/>
